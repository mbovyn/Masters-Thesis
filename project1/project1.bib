journal = {Frontiers in Oncology},
keywords = {microtubule-associated proteins,microtubules,microtubules, tubulin, post-translational modifica,post-translational modifications,stress,tubulin},
number = {June},
pages = {1--19},
pmid = {24995158},
title = {{Microtubules and Their Role in Cellular Stress in Cancer}},
url = {http://journal.frontiersin.org/article/10.3389/fonc.2014.00153/abstract},
volume = {4},
year = {2014}
}
