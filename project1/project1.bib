keywords = {Amoeba,Amoeba: physiology,Animals,Infrared,Infrared Rays,Lasers,Microtubules,Microtubules: physiology,Microtubules: ultrastructure,Mitochondria,Mitochondria: physiology,Mitochondria: ultrastructure,Organelles,Organelles: physiology,Spectrophotometry},
number = {6299},
pages = {346--8},
pmid = {2250707},
title = {{Force generation of organelle transport measured in vivo by an infrared laser trap.}},
url = {http://dx.doi.org/10.1038/348346a0},
volume = {348},
year = {1990}
}
